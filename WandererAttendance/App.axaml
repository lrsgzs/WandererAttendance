<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="WandererAttendance.App"
             RequestedThemeVariant="Default">
    <Application.Styles>
        <StyleInclude Source="/Styles.axaml"/>
    </Application.Styles>
    
    <Application.Resources>
        <FontFamily x:Key="MapleMonoNfCn">avares://WandererAttendance/Assets/Fonts/#Maple Mono NF CN</FontFamily>
        <FontFamily x:Key="FluentSystemIconsResizeable">avares://WandererAttendance/Assets/Fonts/#FluentSystemIcons-Resizable</FontFamily>
    </Application.Resources>
</Application>